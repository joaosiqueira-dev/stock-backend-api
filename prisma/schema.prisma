generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  name String?
  email String @unique
  password String
  CriadoEm DateTime @default(now())

  produtos Produto[]
}

model Produto {
  id String @id @default(uuid())
  name String
  quantity Int
  minQuantity Int
  category String

  userId String
  user User @relation(fields: [userId], references: [id])
}


